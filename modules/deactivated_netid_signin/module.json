{
  "helpers": {
    "classes": {
      "deactivated-netids": "${module.namespace}\\DeactivatedNetidHelper"
    }
  },
  "permissions": {
    "url": {
      "_oldnetid/*": [
        "deny all"
      ],
      "_oldnetid/display": [
        "allow all"
      ],
      "_oldnetid/signin": [
        "allow all"
      ]
    }
  },
  "bootstrap": {
    "pdos": {
      "oldnetids": {
        "dsn": "sqlite:${paths.storage}/oldnetids.sqlite"
      }
    }
  }
}